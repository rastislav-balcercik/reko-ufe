{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "generator-cli": {
      "useDocker": true,
      "version": "6.6.0",
      "generators": {
          "reko-ufe": {
              "generatorName": "typescript-axios",
              "glob": "api/reko-ufe.openapi.yaml",
              "output": "#{cwd}/src/api/reko-ufe",
              "additionalProperties": {
                  "supportsES6": "true",
                  "withInterfaces": true,
                  "enablePostProcessFile": true
              }
          }
      }
  }
}